@page
@model IndexModel
@using ASPPortfolio.Models

@{
  ViewData["Title"] = "Developer Jonas";
}

@section scripts {
  <script type="text/javascript" src="asp/wwwroot/js/3dCube.js" asp-append-version="true"></script>
  <script type="text/javascript" src="asp/wwwroot/js/slideshow.js" asp-append-version="true"></script>
  <script type="text/javascript" src="asp/wwwroot/js/bubbles.js" asp-append-version="true"></script>
}
@section styles {
  <link rel="stylesheet" href="asp/wwwroot/css/3dCube.css" />
  <link rel="stylesheet" href="asp/wwwroot/css/About.css" />
  <link rel="stylesheet" href="asp/wwwroot/css/Apps.css" />
  <link rel="stylesheet" href="asp/wwwroot/css/Intro.css" />
  <link rel="stylesheet" href="asp/wwwroot/css/Project-Slideshow.css" />
  <link rel="stylesheet" href="asp/wwwroot/css/Projects.css" />
  <link rel="stylesheet" href="asp/wwwroot/css/Sections.css" />
  <link rel="stylesheet" href="asp/wwwroot/css/Skills.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css"
    integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
}

<main>
  <section id="intro">
    <img id="background_svg"></img>
    @* <partial name="Components/BubbleStack" /> *@
    <pre class="intro">
<span>Hi, My name is</span>
<span></span><span>Jonas Issa</span>
<span style="width:min-content">I Develop</span> <partial name="Components/3dCube"/>
</pre>
  </section>
</main>

<div id="Content">
  <custom-section id="about" header="About Me">
    <div class="about-content">
      <div class="img-wrap"><img class="m-0" src="asp/wwwroot/images/profile.jpeg"></img>Felsensteg Birg ·
        Lauterbrunnen, Switzerland (2022)</div>Hi, I'm a Software Developer from Sydney.
      I have been coding for the better part of a decade.

      I am highly detail–focused and enjoy performing a mixture of practical work.

      My core specialties in my professional career differentiate vastly from Software, 3D Modeling, Metal machining
      and fabricating Prosthetic limbs.
    </div>
  </custom-section>
  <custom-section id="projects" header="Projects">
      @foreach (var record in @Model.objects)
      {
        <ProjectItem title="@record.Title" preface="@record.Preface" description="@record.Description"
          ext-link="@record.ExtLink" img-src="asp/wwwroot/images/projectImages/@record.ImageName"></ProjectItem>
      }
      <div class="project-list-container">
        <div>
          <a class="prev text-decoration-none" onclick="plusSlides(-1)">❮</a>
        </div>
        @foreach (var record in @Model.objects)
        {
          <ul class="text-nowrap d-flex"><div class="dot justify-content" onclick="currentSlide(@record.Inc)"></div><p>@record.Title</p></ul>
        }
        <div>
          <a class="next text-decoration-none" onclick="plusSlides(1)">❯</a>
        </div>

      </div>
  </custom-section>

  <section class="section-format" id="skills">
    <div class="section-header-wrapper">
      <h1 class="section-header">Skills</h1>
    </div>
    <div class="content-wrapper">
      <div class="skills-wrapper">
        <div class="skills-content">
          <li class="skills-list justify-content-center row gx-5">
            <div class="col-md-12 col-lg-6">
              <h1>SCRIPTING / LANGUAGES</h1>
              <div class="skills-grid-container">
                <ul>
                  <li>C#</li>
                  <li>JavaScript</li>
                  <li>HTML</li>
                  <li>JSON</li>
                  </ul>
                <ul>
                  <li>PYTHON</li>
                  <li>Typescript</li>
                  <li>SQL</li>
                  <li>CSS / SCSS / SASS</li>
                </ul>
                <ul>
                  <li>PHP</li>
                  <li>JQuery</li>
                  <li>Ajax</li>
                </ul>
              </div>
            </div>
            <div class="col-md-12 col-lg-6">
              <h1>TECH STACK</h1>
              <div class="skills-grid-container">
                <ul>
                  <li>.NET Core</li>
                  <li>Dapper</li>
                  <li>Next.js</li>
                  <li>Terraform</li>
                  <li>Tailwind</li>
                </ul>
                <ul>
                  <li>Angular</li>
                  <li>Node.js</li>
                  <li>Docker</li>
                  <li>Webpack
                </ul>
                <ul>
                  <li>Entity Framework</li>
                  <li>Django</li>
                  <li>Nginx</li>
                  <li>Bootstrap</li>
                </ul>
              </div>
            </div>
            <div class="col-md-12 col-lg-6">
              <h1>TOOLS</h1>
              <div class="skills-grid-container">
                <ul>
                  <li>Azure Data Studio</li>
                  <li>Visual Studio</li>
                  <li>APIs</li>
                  <li>Sourcetree</li>
                </ul>
                <ul>
                  <li>Azure DevOps</li>
                  <li>VS Code</li>
                  <li>GIT</li>
                </ul>
                <ul>
                  <li>SSMS</li>
                  <li>Postman</li>
                  <li>Arch Linux</li>
              </ul>
              </div>
            </div>
            <div class="col-md-12 col-lg-6">
              <h1>CONCEPTS</h1>
              <div class="skills-grid-container">
                <ul>
                  <li>CI/CD</li>
                  <li>JIRA</li>
                  <li>HTTP</li>
                  <li>REST</li>
                  <li>Environments</li>
                  <li>Cross-browser Compatibility</li>
                </ul>
                <ul>
                  <li>Agile</li>
                  <li>ClickUp</li>
                  <li>MVP Design</li>
                  <li>Single Page Apps</li>
                  <li>CRM Integration</li>
                  <li>Design Patterns and Principles</li>
                </ul>
                <ul>
                  <li>SCRUM</li>
                  <li>User Story</li>
                  <li>GIT Flow</li>
                  <li>Infrastructure as Code</li>
                  <li>Server Side Rendering</li>
                </ul>
              </div>
            </div>
          </li>
        </div>
      </div>
    </div>
</div>
</section>
</div>
</main>
